apiVersion: portshift.io/v1
kind: PortshiftInstaller
metadata:
  name: portshiftinstaller
  namespace: {{ .Release.Namespace }}
spec:
  managementUrl: {{ .Values.installer.managementUrl }}
  # The cluster id definition in Portshift console. This will be retrieved throught portshift console. this one must be filled with valid cluster id
  portshiftClusterId: {{ .Values.installer.portshiftClusterId }}
